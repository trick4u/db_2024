<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>goalKeep</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>tushar_db</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UIBackgroundModes</key>
    <array>
        <string>remote-notification</string>
		<string>audio</string>
    </array>
    <key>UILocalNotificationSoundName</key>
    <string>notification_sound.wav</string>
    <key>NSCalendarsUsageDescription</key>
    <string>This app requires access to the calendar to schedule event reminders.</string>
    <key>NSRemindersUsageDescription</key>
    <string>This app requires access to reminders to create event reminders.</string>
    <key>NSUserNotificationUsageDescription</key>
    <string>This app requires permission to send you event reminders and updates.</string>
	<key>NSPhotoLibraryUsageDescription</key>
    <string>This app requires access to the photo library to upload images.</string>
    <key>NSCameraUsageDescription</key>
    <string>This app requires access to the camera to take photos.</string>
    <key>NSMicrophoneUsageDescription</key>
    <string>This app requires access to the microphone to record videos.</string>
    <!-- Quick Actions Configuration -->
    <key>UIApplicationShortcutItems</key>
    <array>
        <dict>
            <key>UIApplicationShortcutItemIconFile</key>
            <string>QuickActionHome</string>
            <key>UIApplicationShortcutItemTitle</key>
            <string>Home</string>
            <key>UIApplicationShortcutItemType</key>
            <string>action_home</string>
        </dict>
        <dict>
            <key>UIApplicationShortcutItemIconFile</key>
            <string>QuickActionCalendar</string>
            <key>UIApplicationShortcutItemTitle</key>
            <string>Calendar</string>
            <key>UIApplicationShortcutItemType</key>
            <string>action_calendar</string>
        </dict>
        <dict>
            <key>UIApplicationShortcutItemIconFile</key>
            <string>QuickActionStats</string>
            <key>UIApplicationShortcutItemTitle</key>
            <string>Statistics</string>
            <key>UIApplicationShortcutItemType</key>
            <string>action_stats</string>
        </dict>
        <dict>
            <key>UIApplicationShortcutItemIconFile</key>
            <string>QuickActionAddTask</string>
            <key>UIApplicationShortcutItemTitle</key>
            <string>Add Task</string>
            <key>UIApplicationShortcutItemType</key>
            <string>action_add_task</string>
        </dict>
    </array>
</dict>
</plist>